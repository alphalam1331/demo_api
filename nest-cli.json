{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "assets": [
      {
        "include": "../env/*.env.prod",
        "outDir": "./dist/env",
        "watchAssets": true
      }
    ],
    "deleteOutDir": true
  }
}
